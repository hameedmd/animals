using System;

namespace animalV2
{
    public class TerrestrialAnimalV1 : Animal
    {
        public TerrestrialAnimalV1(int numberOfLegs, string name) : base(numberOfLegs, name)
        {
        }
        public static void Roam()
        {
            Console.WriteLine("I roam the land");
        }
    }
}
